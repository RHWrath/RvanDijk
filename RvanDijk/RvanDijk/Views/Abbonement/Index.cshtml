@model IEnumerable<RvanDijk.Models.VMAbbonement>

@{
    ViewData["Title"] = "Index";
}

<h1>Abbonementen</h1>

<p>
    
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vergoeding)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prijs)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vergoeding)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prijs)
            </td>
        </tr>
}
    </tbody>
</table>
